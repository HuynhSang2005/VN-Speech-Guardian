{
  "name": "@vn-speech-guardian/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend React 19 + Vite 7 cho VN Speech Guardian - Speech-to-Text vÃ  harmful content detection",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "cross-env ANALYZE_BUNDLE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "preview": "vite preview --host",
    "analyze": "npm run build:analyze && node -r tsx/register scripts/bundle-analysis.ts",
    "analyze:size": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "analyze:deps": "npx bundlephobia-cli --package-json",
    "analyze:unused": "npx depcheck",
    "analyze:duplicates": "npx duplicate-package-checker node_modules",
    "bundle:visualize": "npm run build:analyze && open dist/bundle-analysis/stats.html",
    "bundle:network": "npm run build:analyze && open dist/bundle-analysis/network.html",
    "bundle:report": "npm run analyze && open bundle-analysis/bundle-report.html",
    "bundle:track": "npm run analyze && git add bundle-analysis/bundle-metrics.csv",
    "performance:monitor": "node -r tsx/register scripts/performance-monitor.ts",
    "performance:ci": "npm run performance:monitor && node -e \"const report = require('./bundle-analysis/cicd-report.json'); if (!report.thresholds.bundleSizePass) process.exit(1);\"",
    "generate:api-types": "copyfiles \"../gateway-nestjs/public/openapi.json\" \"src/schemas/generated/\" && npx openapi-typescript src/schemas/generated/openapi.json -o src/schemas/generated/api-types.ts",
    "generate:orval": "orval --config orval.config.ts",
    "generate:api": "npm run generate:api-types && npm run generate:orval",
    "generate:watch": "nodemon --watch \"../gateway-nestjs/public/openapi.json\" --exec \"npm run generate:api\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:performance": "vitest run --reporter=verbose src/tests/performance/",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
